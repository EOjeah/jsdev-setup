  "description": "",
  "private": true,
  "scripts": {
    "build": "babel lib -d dist",
    "prestart": "babel-node dist/startMessage.js",
    "start": "concurrently --kill-others-on-fail \"npm:test:watch\" \"babel-node dist/server.js\" \"npm:lint:watch\"",
    "lint": "esw webpack.config.* src lib --color",
    "lint:watch": "npm run lint -- --watch",
    "localtunnel": "lt --port 3000 --subdomain manny",
    "share": "npm-run-all --parallel start localtunnel",
    "test": "mocha \"src/**/*.test.js\"",
    "test:watch": "npm run test -- --watch"
  },
